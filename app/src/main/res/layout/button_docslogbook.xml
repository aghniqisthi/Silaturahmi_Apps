<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_fileLogbook"
        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="4dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="Lihat Dokumen"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        android:textColor="#458654"
        android:textSize="14sp"
        app:icon="@drawable/outline_text_snippet_24"
        app:iconPadding="10dp"
        app:layout_constraintEnd_toStartOf="@+id/delete_logbook"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="#458654" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/delete_logbook"
        style="@style/Widget.Material3.Button.IconButton"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginHorizontal="6dp"
        android:layout_weight="3"
        android:backgroundTint="#FF3B30"
        android:fontFamily="@font/poppins_regular"
        app:cornerRadius="6dp"
        app:icon="@drawable/rounded_delete_24"
        app:iconSize="24dp"
        app:iconTint="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/btn_fileLogbook"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btn_fileLogbook" />

</androidx.constraintlayout.widget.ConstraintLayout>